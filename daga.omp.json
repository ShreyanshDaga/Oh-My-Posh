{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "version": 2,
  "final_space": true,
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "path",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "#FFA500",
          "background": "transparent",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b0",
          "template": " \ue5ff {{ .Path }} ",
          "properties": {
            "style": "folder"
          }
        },
        {
          "type": "git",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "#dddddd",
          "foreground_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#fffb38{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#ff4500{{ end }}",
            "{{ if gt .Ahead 0 }}#08f7f4{{ end }}",
            "{{ if gt .Behind 0 }}#f83507{{ end }}"
          ],
          "background": "transparent",
          "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Staging.Changed) (.Working.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0}} \uf692 {{ .StashCount }}{{ end }}{{ if gt .WorktreeCount 0}} \uf1bb {{ .WorktreeCount }}{{ end }} ",
          "properties": {
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "display_upstream_icon": true
          }
        },
        {
          "type": "dotnet",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "background": "transparent",
          "foreground": "#ac7ce5",
          "template": " \ue70c {{ if .Unsupported }}\uf071{{ else }}{{ .Full }}{{ end }} ",
          "properties": {
            "fetch_version": true,
            "prefix": " îœŒ "
          }
        },
        {
          "type": "root",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "#ffff66",
          "background": "transparent",
          "template": " \uf0e7 "
        },
        {
          "type": "exit",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "background": "transparent",
          "foreground": "#02ff00",          
          "foreground_templates": [
            "{{ if gt .Code 0 }}#f1184c{{ end }}"
          ],
          "trailing_diamond": "\ue0b4",
          "template": " \ue23a{{ if gt .Code 0 }} \uf00d{{ else }} \uf42e{{ end }} ",
          "properties": {
            "always_enabled": true
          }
        }
      ]
    },{
        "type": "rprompt",
        "segments": [
            {
                "type": "executiontime",
                "style": "powerline",
                "powerline_symbol": "\uE0C2",
                "foreground": "#8800dd",
                "background": "transparent",
                "template": "<#fefefe>\ufbab</> <i>{{ .FormattedMs }}</i>",
                "properties": {
                  "threshold": 500,
                  "style": "amarillo"
                }
              }
        ]
    }
  ]
}
